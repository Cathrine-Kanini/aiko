{
  "version": 2,
  "builds": [
    {
      "src": "aiko/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/aiko/main.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production",
    "LLM_PROVIDER": "google",
    "LLM_MODEL": "gemini-1.5-flash",
    "DEBUG": "False",
    "LOG_LEVEL": "INFO"
  }
}